@using Sitecore.Mvc
@using Sitecore.Mvc.Analytics.Extensions

@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en" xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>@Html.Sitecore().Field("title", new { DisableWebEdit = true })</title>
    <link rel="stylesheet" href="~/Content/bootstrap.min.css" />
    <link rel="stylesheet" href="~/Content/styles.css" />
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.0.10/css/all.css" integrity="sha384-+d0P83n9kaQMCwj8F4RJB66tzIwOKmrdb46+porD/OvrJ+37WqIM7UoBtwHO6Nlg" crossorigin="anonymous">
    <link href="https://fonts.googleapis.com/css?family=Raleway:400,500,600,700,800,900" rel="stylesheet">

    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/iehp.js"></script>
    <script src="https://player.vimeo.com/api/player.js"></script>

    @Html.Sitecore().VisitorIdentification()
</head>
<body>
    <div class="container-fluid">
        <div class="col-24 col-md-24 col-lg-20 col-xxl-14 mx-auto">
            @Html.Sitecore().Placeholder("navCtrl")
        </div>
    </div>

    <div id="imageSlider" class="row">
        @RenderPage("~/Views/Shared/_StickySocial.cshtml")

        @Html.Sitecore().Placeholder("memberHero")
    </div>

    <div class="container-fluid">
        <div class="col-14 mx-auto row justify-content-between">
            <div class="col-24 col-sm-8">
                @Html.Sitecore().Placeholder("memberLogin")
            </div>

            <div class="col-24 col-sm-7 planPadding magentaBackground white">
                @Html.Sitecore().Placeholder("planOne")
            </div>

            <div class="col-24 col-sm-8 planPadding magentaBackground white">
                @Html.Sitecore().Placeholder("planTwo")
            </div>

            @*<div class="col-11">
                @Html.Sitecore().Placeholder("memberInfo")
            </div>*@
        </div>
    </div>

    <div class="container-fluid">
        <div class="col-14 mx-auto row itemSep justify-content-between">
            @*<div id="plansWrapper" class="col-17 row magentaBackground white">
            <div class="col-24 fmPlansHeader">
                <h4 class="ourPlans">Our Plans</h4>
            </div>
        </div>*@

            <div class="col-12 ctaWidgets vertDivider">
                @Html.Sitecore().Placeholder("cahCTA")
            </div>

            <div class="col-12 plansWidget">
                @Html.Sitecore().Placeholder("plansWidget")
            </div>

            @*<div id="plansCTAWrapper" class="col-24 row">
            @Html.Sitecore().Placeholder("plansCTA")
