                                <!--Tab Content Goes Here-->
                            </div>
                        </div>

                        <div class="tab-pane" id="hwTab6Tab">
                            <div id="hwTab6Content" class="tabWrapper row">
                                <!--Tab Content Goes Here-->
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="col-14 mx-auto row topPadding justify-content-between white">
            <div class="col-5 allPadding greenBackground">
                @Html.Sitecore().Placeholder("hwEvent")
            </div>

            <div class="col-5 ctaWidgets greenBackground">
                @Html.Sitecore().Placeholder("hwCTA")
            </div>
        </div>
    </div>

    <div class="container-fluid">
        <div class="col-14 mx-auto row itemSep justify-content-between white">

            <div class="col-17 magentaBackground row">
                <div class="col-24 mrHeader"><h4>Member Resources</h4></div>
                @Html.Sitecore().Placeholder("mrWidget")
            </div>
        </div>
    </div>

    <div id="footer" class="container-fluid">
        <div id="footerLinks" class="col-md-24 col-lg-20 col-xxl-14 mx-auto row">
            @Html.Sitecore().Placeholder("footerLinks")
        </div>
    </div>

    <div id="extraFooter" class="container-fluid">
        <div class="col-md-24 col-lg-20 col-xxl-14 mx-auto row topPadding text-center">
            @Html.Sitecore().Placeholder("extraFooterLinks")
        </div>
    </div>



    <script src="~/Scripts/bootstrap.min.js"></script>
