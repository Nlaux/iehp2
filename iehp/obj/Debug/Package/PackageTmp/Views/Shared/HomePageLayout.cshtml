@using Sitecore.Mvc
@using Sitecore.Mvc.Analytics.Extensions

@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en" xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>@Html.Sitecore().Field("title", new { DisableWebEdit = true })</title>
    <link rel="stylesheet" href="~/Content/bootstrap.min.css" />
    <link rel="stylesheet" href="~/Content/styles.css" />
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/iehp.js"></script>

    @Html.Sitecore().VisitorIdentification()
</head>
<body>
    <div class="container-fluid">
        <div class="col-14 mx-auto">
            <div id="subNavWrapper"class="col-24 row text-right">
               
            </div>

            <div class="row">
                <div class="col-3 text-left">
                    <img class="smallLogo" src="https://ww3.iehp.org/Images/IEHP_LOGO_w194xh106.png" />
                </div>

                <div id="mainNavWrapper" class="col-21 row text-right">

                </div>
            </div>
        </div>
    </div>

    <div id="imageSlider" class="row">
        @Html.Sitecore().Placeholder("imageSlider")
    </div>

    <div class="container-fluid">
        <div id="mainTabsNav" class="col-14 mx-auto">
            <ul class="nav nav-tabs nav-fill">
                <li class="nav-item">
                    <a id="doctorTab" class="mainTab nav-link active" href="#" data-toggle="tab">Doctor</a>
                </li>
                <li class="nav-item">
                    <a id="urgentTab" class="mainTab nav-link" href="#" data-toggle="tab">Urgent Care</a>
                </li>
                <li class="nav-item">
                    <a id="pharmacyTab" class="mainTab nav-link" href="#" data-toggle="tab">Pharmacy</a>
                </li>
            </ul>

            <div class="tab-content clearfix">
                <div class="tab-pane active" id="doc">
                    <div class="tabWrapper row">
                        <!--Tab Content Goes Here-->
                    </div>
                </div>

                <div class="tab-pane" id="urg">
                    <div class="tabWrapper row">
                        <!--Tab Content Goes Here-->
                    </div>
                </div>

                <div class="tab-pane" id="pharm">
                    <div class="tabWrapper row">
                        <!--Tab Content Goes Here-->
                    </div>
                </div>
            </div>

            <div id="plansWrapper" class="row">
                <div class="col-12 extraBottom">
                    @Html.Sitecore().Placeholder("mediPlan")
                </div>

                <div class="col-12">
                    @Html.Sitecore().Placeholder("dualChoicePlan")
                </div>
            </div>

        </div>
    </div>

    <div id="footer" class="container-fluid">
        <div id="footerLinks" class="col-14 mx-auto row">
            
        </div>
    </div>



    <script src="~/Scripts/bootstrap.min.js"></script>
</body>
</html>