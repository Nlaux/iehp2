@using Sitecore.Mvc
@using Sitecore.Mvc.Analytics.Extensions

@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en" xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>@Html.Sitecore().Field("title", new { DisableWebEdit = true })</title>
    <link rel="stylesheet" href="~/Content/bootstrap.min.css" />
    <link rel="stylesheet" href="~/Content/styles.css" />
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/iehp.js"></script>

    @Html.Sitecore().VisitorIdentification()
</head>
<body>
    <div class="container-fluid">
        <div class="col-14 mx-auto">
            <div class="col-24 row text-right">
                @Html.Sitecore().Placeholder("subNav")
            </div>

            <div class="row">
                <div class="col-3 text-left">
                    <img class="smallLogo" src="https://ww3.iehp.org/Images/IEHP_LOGO_w194xh106.png" />
                </div>

                <div class="col-21 text-right">
                    @Html.Sitecore().Placeholder("mainNav")
                </div>
            </div>
        </div>
    </div>

    <div id="imageSlider" class="row">
        @Html.Sitecore().Placeholder("imageSlider")
    </div>

    <div class="container-fluid">
        <div id="mainTabsNav" class="col-14 mx-auto">
            <ul class="nav nav-tabs nav-fill">
                <li class="nav-item">
                    <a id="doctorTab" class="mainTab nav-link active" href="#" data-toggle="tab">Doctor</a>
                </li>
                <li class="nav-item">
                    <a id="urgentTab" class="mainTab nav-link" href="#" data-toggle="tab">Urgent Care</a>
                </li>
                <li class="nav-item">
                    <a id="pharmacyTab" class="mainTab nav-link" href="#" data-toggle="tab">Pharmacy</a>
                </li>
            </ul>

            <div class="tab-content clearfix">
                <div class="tab-pane active" id="doc">
                    <div class="row">
                        <div id="fadCTA" class="col-12">

                        </div>

                        <div id="fadForm" class="col-12">
                            @Html.Sitecore().Placeholder("fadForm")
                        </div>

                        <div class="newsArticles col-8">
                            @Html.Sitecore().Placeholder("latestNews")
                        </div>

                        <div id="eventCalendarBlockDoctor" class="col-16 extraBottom row">
                            <div id="loading"></div>
                        </div>

                        <div id="eventCalendarBlockUrgent" class="col-16 extraBottom row">
                            <div id="loading"></div>
                        </div>

                        <div id="eventCalendarBlockPharmacy" class="col-16 extraBottom row">
                            <div id="loading"></div>
                        </div>

                        <div class="col-12 extraBottom">
                            @Html.Sitecore().Placeholder("mediPlan")
                        </div>

                        <div class="col-12">
                            @Html.Sitecore().Placeholder("dualChoicePlan")
                        </div>
                    </div>
                </div>

                <div class="tab-pane" id="urg">
                    <div class="row">
                        <div id="urgCTA" class="col-12">
                            @Html.Sitecore().Placeholder("urgCTA")
                        </div>

                        <div id="urgSearch" class="col-12">
                            @Html.Sitecore().Placeholder("urgForm")
                        </div>

                        <div class="newsArticles col-8">
                            @Html.Sitecore().Placeholder("latestNews")
                        </div>

                        <div id="eventCalendarBlockDoctor" class="col-16 extraBottom row">
                            <div id="loading"></div>
                        </div>

                        <div id="eventCalendarBlockUrgent" class="col-16 extraBottom row">
                            <div id="loading"></div>
                        </div>

                        <div id="eventCalendarBlockPharmacy" class="col-16 extraBottom row">
                            <div id="loading"></div>
                        </div>

                        <div class="col-12 extraBottom">
                            @Html.Sitecore().Placeholder("mediPlan")
                        </div>

                        <div class="col-12">
                            @Html.Sitecore().Placeholder("dualChoicePlan")
                        </div>
                    </div>
                </div>

                <div class="tab-pane" id="pharm">
                    <div class="row">
                        <div id="pharmCTA" class="col-12">
                            @Html.Sitecore().Placeholder("pharmCTA")
                        </div>

                        <div id="pharmSearch" class="col-12">
                            @Html.Sitecore().Placeholder("pharmForm")
                        </div>

                        <div class="newsArticles col-8">
                            @Html.Sitecore().Placeholder("latestNews")
                        </div>

                        <div id="eventCalendarBlockDoctor" class="col-16 extraBottom row">
                            <div id="loading"></div>
                            @Html.Sitecore().Placeholder("upcomingEvents")
                        </div>

                        <div id="eventCalendarBlockUrgent" class="col-16 extraBottom row">
                            <div id="loading"></div>
                            @Html.Sitecore().Placeholder("upcomingEvents")
                        </div>

                        <div id="eventCalendarBlockPharmacy" class="col-16 extraBottom row">
                            <div id="loading"></div>
                            @Html.Sitecore().Placeholder("upcomingEvents")
                        </div>

                        <div class="col-12 extraBottom">
                            @Html.Sitecore().Placeholder("mediPlan")
                        </div>

                        <div class="col-12">
                            @Html.Sitecore().Placeholder("dualChoicePlan")
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div id="footer" class="container-fluid">
        <div class="col-14 mx-auto row">
            <div id="primaryFooterLinks" class="col-20 row">
                <div class="col-4 topPadding">
                    @Html.Sitecore().Placeholder("footerLinksCol1")
                </div>

                <div class="col-9 topPadding">
                    @Html.Sitecore().Placeholder("footerLinksCol2")
                </div>

                <div class="col-5 topPadding">
                    @Html.Sitecore().Placeholder("footerLinksCol3")
                </div>

                <div class="col-4 topPadding">
                    @Html.Sitecore().Placeholder("footerLinksCol4")
                </div>
            </div>

            <div id="secondaryFooterLinks" class="col-4 topPadding">
                @Html.Sitecore().Placeholder("footerLinksCol5")
            </div>

            <div id="extraFooter" class="col-24">
                @Html.Sitecore().Placeholder("extraFooter")
            </div>
        </div>
    </div>



    <script src="~/Scripts/bootstrap.min.js"></script>
</body>
</html>